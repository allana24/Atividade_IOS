// Tabuada do  3 //

var i = 1;
while (i <= 10) {
  console.log("3 x " + i + " = " + (3 * i));
  i++;
}

// Tabuada do 4 //

var i = 1;
while (i <= 10) {
  console.log("4 x " + i + " = " + (4 * i));
  i++;
}

